  height: 40rpx;
  display:flex;
  justify-content: center;
}
.listItem .rankingView .ranking image{
  height: 100%;
  width: 100%;
}
.listItem .rankingView  .rankingNum{
  width: 32rpx;
  display:flex;
  justify-content: center;
}
.listItem .rankingView .headPortrait{
  width:180rpx;
  height:100%;
  display: flex;
  align-items: center;
  font-family: PingFangSC-Medium;
