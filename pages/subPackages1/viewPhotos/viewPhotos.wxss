  margin-right: 61rpx;
}
.list .front{
  color: #ff5700;
}
.listItem .rankingView{
  width: 272rpx;
  height:100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.listItem .rankingView .ranking{
