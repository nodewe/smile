	font-size: 24rpx;
	font-weight: normal;
	font-stretch: normal;
	color: #000000;
}
.rankingListWindow .list{
  width: 100%;
    box-sizing: border-box;
    height: 582rpx;
    padding: 30rpx 0rpx 0rpx 19rpx;
}
.rankingimg{
  width:50rpx;
  height:50rpx;
  border-radius:50%;
  overflow: hidden;
}
.list .listItem{
  height: 64rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-family: SourceHanSansCN-Medium;
	font-size: 24rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0rpx;
