.container .rankingListWindow{
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
	height: 678rpx;
	background-color: #ffffff;
  border-top-left-radius: 42rpx;
    border-top-right-radius: 42rpx;
  padding: 43rpx 0rpx 0rpx 42rpx;
  box-sizing: border-box;
