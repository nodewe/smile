  width: 50rpx;
  height: 50rpx;
  border-radius:50%;
  overflow: hidden;
  margin-right: 18rpx;
}

.container .moreRelayWindow{
  position:fixed;
  top:0;
  right:0;
  width:678rpx;
  height:100%;
  background-color: #111111;
  display:flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
}
.container .moreRelayWindow .header{
  /* height: 400rpx; */
  width: 100%;
  padding: 53rpx 21rpx 23rpx 22rpx;
  display: flex;
  justify-content: space-between;
  align-items:center;
  box-sizing: border-box;
}
.moreRelayWindow .header .search-btn{
  width:100rpx;
  height:50rpx;
  color:white;
  font-size:26rpx;
  display:flex;
  justify-content:center;
  align-items:center;
}
.moreRelayWindow .header .inputView{
  width: 635rpx;
  height: 64rpx;
  margin-right:10rpx;
  border-radius: 32rpx;
  background-color: rgba(255,255,255,0.1);
  display: flex;
  justify-content: center;
  align-items: space-between;
  position: relative;
  font-family: PingFangSC-Regular;
	font-size: 28rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 35rpx;
	letter-spacing: 0rpx;
  color: #ffffff;
}
.header .inputView input{
  width: 100%;
  height: 100%;
  padding: 0 32rpx;
}
.header .inputView .defaultView{
  position: absolute;
  top: 50%;
  margin-top: -18rpx;
  width: 154rpx;
  height: 28rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.header .inputView .defaultView.hide{
  display:none;
}
.inputView .defaultView image{
  width: 28rpx;
  height: 28rpx;
}
.container .moreRelayWindow .morePhotos{
  /* width:100%; */
  height: 100%;
  /* margin-top:400rpx; */
  padding:0 21rpx;
  box-sizing:border-box;
  z-index: 200;
}
.moreRelayWindow .morePhotos .row{
  width: 635rpx;
  /* height: 400rpx; */
  flex-wrap: wrap;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom:17rpx;
}
.morePhotos .row .relayPhotoView{
  position: relative;
  width:310rpx;
  height:550rpx;
  border-radius: 20rpx;
  position: relative;
  margin-bottom:20rpx;
  overflow: hidden;
}
.row .relayPhotoView image{
  width:100%;
  height: 100%;
}
.row .relayPhotoView .likeNumView{
  position:absolute;
  bottom:31rpx;
  right:18rpx;
  height:24rpx;
  line-height: 26rpx;
  font-family: PingFangSC-Regular;
	font-size: 24rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0rpx;
  color: #ffffff;
  display: flex;
    justify-content: space-between;
    align-items: unset;
}
.relayPhotoView .likeNumView .heart{
  width: 30rpx;
  height: 27rpx;
  margin-right: 7rpx;
  z-index: 20;
}
.row .relayPhotoView .shadow{
  position: absolute;
  bottom: 0;
  left: 0;
  width: 310rpx;
	height: 80rpx;
  background-image: linear-gradient(180deg, 
		rgba(0,0,0,0) 0%, 
		rgba(0,0,0,0.8) 100%);
	border-radius: 0rpx 0rpx 20rpx 20rpx;
}

.container .shareModel{
  height: 100%;
  width: 100%;
  position: fixed;
  background-color:rgba(0, 0, 0, .5);
  z-index: 2;
  top: 0;
  left: 0;
  display:flex;
  justify-content: center;
  align-items: center;
}
.container .show{
  display: flex;
}
.container .showBlock{
  display: block;
}
.container .hide{
  display: none;
}
.container .shareModel .shareWindow{
  width: 600rpx;
  /* height: 1017rpx; */
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  transform: scale(0.75);
}
.shareModel .shareWindow .card{
  width: 530rpx;
  height: 1074rpx;
  margin-top:25rpx;
  padding: 25rpx 25rpx 20rpx 25rpx;
  box-sizing: border-box;
	background-color: #ffffff;
  border-radius: 15rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
}
.shareWindow .card .sharePhoto{
  width:360rpx;
  height:640rpx;
  transform:scale(1.3) translateY(75rpx);
  /* overflow: hidden; */
}
.shareWindow .card .sharePhoto image{
  width: 100%;
  height: 100%;
}
.shareWindow .card .publicity{
  width: 470rpx;
  height:187rpx;
  margin: 21rpx 47rpx 0rpx 40rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.card .publicity .tagline{
  height: 145rpx;
  width: 240rpx;
  font-family: FZZDHJW--GB1-0;
	font-size: 28rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 40rpx;
	letter-spacing: 0rpx;
  color: #333333;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.publicity .tagline .redNum{
  font-family: FZZDHJW--GB1-0;
	font-size: 36rpx;
	font-weight: normal;
	letter-spacing: 0rpx;
	color: #fc3e35;
}
.card .publicity .QRcode{
  width: 178rpx;
  height: 100%;
  /* display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center; */
  text-align:center;
  font-family: PingFangSC-Regular;
	font-size: 24rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 40rpx;
	letter-spacing: 0rpx;
	color: #666666;
}
.QRcode image{
  width: 144rpx;
  height: 143rpx;
  margin:0rpx 14rpx 0rpx 8rpx;
}
.shareModel .shareWindow .shareBtns{
  width: 100%;
  height: 79rpx;
  margin-top:30rpx;
  display: flex;
  justify-content: space-between;
  font-family: PingFangSC-Medium;
	font-size: 34rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 61rpx;
	letter-spacing: 1rpx;
	color: #ffffff;
}
.shareWindow .shareBtns .shareBtn{
  width: 291rpx;
	height: 79rpx;
	background-image: linear-gradient(180deg, 
		#57e680 0%, 
		#00bf55 100%), 
	linear-gradient(
		#fc8535, 
		#fc8535);
	background-blend-mode: normal, 
		normal;
	box-shadow: 0rpx 3rpx 7rpx 0rpx 
		rgba(8, 150, 58, 0.56);
  border-radius: 40rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0rpx 28rpx;
  box-sizing: border-box;
  font-family: PingFangSC-Medium;
	font-size: 34rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 61rpx;
	letter-spacing: 1rpx;
  color: #ffffff;
  margin:0;
}

.shareBtns .shareBtn image{
  width: 54rpx;
  height: 45rpx;
}
.shareWindow .shareBtns .saveBtn{
  width: 291rpx;
	height: 79rpx;
	background-image: linear-gradient(180deg, 
		#fcaf35 0%, 
		#fc8535 100%), 
	linear-gradient(
		#fc8535, 
		#fc8535);
	background-blend-mode: normal, 
		normal;
	box-shadow: 0rpx 3rpx 7rpx 0rpx 
		rgba(226, 116, 9, 0.56);
  border-radius: 40rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.careLike{
  width:150rpx;
  height:150rpx;
  position:fixed;
  opacity: 1;
  transform: rotate(15deg);
  transition:opacity .3s linear;
}
.mask{
  width:100%;
  height:100vh;
  position:fixed;
  top:0;
  left:0;
  background-color:rgba(0, 0, 0,0);
}
.fixed{
  width:100vw;
  height:100vh;
  position: fixed;
  top:0;
  right:-9999px;
}
.loading{
  position:absolute;
  top:0;
  left:0;
  width:100%;
  height:100%;
  background-color:rgba(0, 0, 0, 0);
  z-index:21;
  display:flex;
  justify-content:center;
  align-items:center;
  color:white;
}
.loading .loading-center{
  display:flex;
  flex-direction:column;
  justify-content:center;
  align-items:center;
}
.loading image{
  width:80rpx !important;
  height:80rpx !important;
  margin-bottom:10rpx;
}
.tips{
  position:fixed;
  bottom:20rpx;
  left:0;
  display:flex;
  justify-content:center;
  align-items:center;
  color:white;
  z-index:-1;
  width:100vw;
}
.tips::after,.tips::before{
  content:'';
  width:30rpx;
  height:5rpx;
  background-color:white;
}
.tips view{
  font-size:32rpx;
  margin:0 20rpx;
}
.tipsTop{
  top:70rpx;
  bottom:auto;
}